{
  "name": "@brybrant/fade-scroll",
  "description": "Fade Scroll is a cosmetic module which adds subtle gradient masks to the overflow of scrollable content.",
  "keywords": ["Fade", "Scroll", "FadeScroll", "Gradient", "Mask"],
  "author": "brybrant",
  "license": "GPL-3.0-only",
  "version": "1.0.1",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist/fade-scroll.css",
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.mjs"
  ],
  "scripts": {
    "dev": "env NODE_ENV=development gulp dev",
    "build": "env NODE_ENV=production gulp build && dts-bundle-generator --umd-module-name FadeScroll --no-banner -o dist/index.d.ts src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brybrant/fade-scroll.git"
  },
  "homepage": "https://brybrant.github.io/fade-scroll/",
  "devDependencies": {
    "@brybrant/configs": "github:brybrant/configs",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "browser-sync": "^3.0.2",
    "chalk": "^5.3.0",
    "dts-bundle-generator": "^9.5.1",
    "ejs": "^3.1.10",
    "eslint": "^9.10.0",
    "gulp": "^5.0.0",
    "gulp-postcss": "^10.0.0",
    "gulp-sass": "^5.1.0",
    "postcss": "^8.4.47",
    "postcss-reporter": "^7.1.0",
    "rollup": "^4.21.3",
    "sass": "^1.77.8",
    "typescript-eslint": "^8.6.0"
  },
  "browserslist": [
    "Chrome >= 48",
    "Edge >= 79",
    "ff >= 53",
    "Opera >= 46",
    "Safari >= 13",
    "and_chr >= 25",
    "and_ff >= 53",
    "op_mob >= 80",
    "ios_saf >= 7",
    "Samsung >= 4",
    "not dead"
